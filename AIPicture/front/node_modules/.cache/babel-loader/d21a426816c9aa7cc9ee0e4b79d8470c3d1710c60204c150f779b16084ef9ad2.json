ype=\"primary\" round>确定</el-button></router-link>\r\n        </el-dialog>\r\n    </span>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data(){\r\n    return{\r\n      dialogVisible:false,\r\n      user_for_register:{\r\n        username:\"\",\r\n        password:\"\",\r\n      },\r\n      password:\"\",\r\n    }\r\n  },\r\n  methods:{\r\n    handleClose(done) {\r\n      this.$confirm('确认关闭？')\r\n          .then(() => {\r\n            done();\r\n          })\r\n          .catch(() => {});\r\n    },\r\n\r\n    register(){\r\n      if (this.user_for_register.password == this.password){\r\n        axios({\r\n          url:\"http://localhost:8080/register\",\r\n          data:this.user_for_register,\r\n          method:\"POST\",\r\n        })\r\n        alert(\"注册成功！\");\r\n      }else {\r\n        alert(\"两次密码不一致，请重试！\");\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}