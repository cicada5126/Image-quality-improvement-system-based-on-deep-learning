iew(file) {\n      console.log(file);\n    }\n  }\n}\n</script>\n\n<style>\n.el-header, .el-footer {\n  background-color: #B3C0D1;\n  color: #333;\n  text-align: center;\n  line-height: 60px;\n}\n\n.el-aside {\n  background-color: #D3DCE6;\n  color: #333;\n  text-align: center;\n  line-height: 200px;\n}\n\n.el-main {\n  background-color: #E9EEF3;\n  color: #333;\n  text-align: center;\n  line-height: 100%;\n}\n\nbody > .el-container {\n  margin-bottom: 40px;\n}\n\n.el-container:nth-child(5) .el-aside,\n.el-container:nth-child(6) .el-aside {\n  line-height: 100%;\n}\n\n.el-container:nth-child(7) .el-aside {\n  line-height: 320px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}