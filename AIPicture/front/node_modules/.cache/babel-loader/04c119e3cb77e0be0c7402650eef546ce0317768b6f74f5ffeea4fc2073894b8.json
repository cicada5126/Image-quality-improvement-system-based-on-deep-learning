{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      circleUrl: \"https://cube.elemecdn.com/3/7c/3ea6beec64369c2642b92c6726f1epng.png\",\n      user_for_login: {\n        username: \"\",\n        password: \"\"\n      },\n      dialogVisible: false\n    };\n  },\n  methods: {\n    handleClose(done) {\n      this.$confirm('确认关闭？').then(() => {\n        done();\n      }).catch(() => {});\n    },\n    login() {\n      if (sessionStorage.getItem(\"isLogin\") == \"true\") {\n        alert(\"请勿重复登录\");\n        this.dialogVisible = true;\n      } else {\n        axios({\n          url: \"http://localhost:8080/login\",\n          data: this.user_for_login,\n          method: \"POST\"\n        }).then(res => {\n          console.log(res.data);\n          if (res.data == \"1\") {\n            sessionStorage.setItem(\"isLogin\", \"true\");\n            this.dialogVisible = true;\n            //sessionStorage.clear();//删除保存的登录信息，注销登录\n          } else {\n            alert(\"用户名或密码不正确\");\n          }\n        });\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AA2BA;AAEA;EACAA;IACA;MACAC;MACAC;QACAC;QACAC;MACA;MAEAC;IACA;EACA;EACAC;IACAC;MACA,uBACAC;QACAC;MACA,GACAC;IACA;IAEAC;MACA;QACAC;QACA;MACA;QACAC;UACAC;UACAd;UACAe;QACA;UACAC;UACA;YACAC;YACA;YACA;UACA;YACAL;UACA;QACA;MACA;IACA;EACA;AACA","names":["data","circleUrl","user_for_login","username","password","dialogVisible","methods","handleClose","then","done","catch","login","alert","axios","url","method","console","sessionStorage"],"sourceRoot":"src/views","sources":["Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"text-center\">\r\n    <div><h1>登 录</h1></div>\r\n    <div class=\"demo-basic--circle\">\r\n      <div class=\"block\"><el-avatar :size=\"120\" :src=\"circleUrl\"></el-avatar></div>\r\n      <div class=\"block\">\r\n        <el-avatar :size=\"size\" :src=\"circleUrl\"></el-avatar>\r\n      </div>\r\n    </div>\r\n    <br><span class=\"text-center\">账 号</span><input type=\"text\" v-model=\"user_for_login.username\"/>\r\n    <br>\r\n    <br><span class=\"text-center\">密 码</span><input type=\"password\" v-model=\"user_for_login.password\"/>\r\n    <br>\r\n    <br>\r\n    <span slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button type=\"primary\" @click=\"login \">确 定</el-button>\r\n      <el-dialog title=\"提示\" :visible.sync=\"dialogVisible\" width=\"30%\" :before-close=\"handleClose\">\r\n        <div>登录成功</div>\r\n        <br>\r\n        <br>\r\n        <router-link to=\"/System\"><el-button type=\"primary\" round>确定</el-button></router-link>\r\n        </el-dialog>\r\n    </span>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data(){\r\n    return{\r\n      circleUrl: \"https://cube.elemecdn.com/3/7c/3ea6beec64369c2642b92c6726f1epng.png\",\r\n      user_for_login:{\r\n        username:\"\",\r\n        password:\"\",\r\n      },\r\n\r\n      dialogVisible:false,\r\n    }\r\n  },\r\n  methods:{\r\n    handleClose(done) {\r\n      this.$confirm('确认关闭？')\r\n          .then(() => {\r\n            done();\r\n          })\r\n          .catch(() => {});\r\n    },\r\n\r\n    login(){\r\n      if (sessionStorage.getItem(\"isLogin\") == \"true\"){\r\n        alert(\"请勿重复登录\");\r\n        this.dialogVisible = true;\r\n      }else {\r\n        axios({\r\n          url: \"http://localhost:8080/login\",\r\n          data: this.user_for_login,\r\n          method: \"POST\",\r\n        }).then(res => {\r\n          console.log(res.data);\r\n          if (res.data == \"1\") {\r\n            sessionStorage.setItem(\"isLogin\", \"true\");\r\n            this.dialogVisible = true;\r\n            //sessionStorage.clear();//删除保存的登录信息，注销登录\r\n          } else {\r\n            alert(\"用户名或密码不正确\");\r\n          }\r\n        })\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies