{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      image: '',\n      dialogImageUrl: '',\n      dialogVisible: false,\n      urls: []\n    };\n  },\n  methods: {\n    getImgUrl(img) {\n      //这里的路径注意自己的后端Controller路径\n      return `http://localhost:8080/common/download?name=${img}`;\n    },\n    handleRemove(file, fileList) {\n      console.log(file, fileList);\n    },\n    handlePictureCardPreview(file) {\n      this.dialogImageUrl = file.url;\n      this.dialogVisible = true;\n    }\n  }\n};","map":{"version":3,"mappings":"AAkCA;AAEA;EACAA;EAEAC;IACA;MACAC;MACAC;MACAC;MACAC;IAGA;EACA;EAEAC;IACAC;MACA;MACA;IACA;IAEAC;MACAC;IACA;IACAC;MACA;MACA;IACA;EACA;AACA","names":["name","data","image","dialogImageUrl","dialogVisible","urls","methods","getImgUrl","handleRemove","console","handlePictureCardPreview"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <div class=\"bg-white : text-center \">\n    <el-image\n        style=\"height:80\"\n        :src=\"getImgUrl(row.image)\"\n    >\n    </el-image>\n\n    <el-container>\n      <el-header>Header</el-header>\n      <el-container>\n        <el-aside width=\"200px\">\n          <el-upload\n              action=\"http://localhost:8080/system/picture/upload\"\n              list-type=\"picture-card\"\n              :on-preview=\"handlePictureCardPreview\"\n              :on-remove=\"handleRemove\"\n              :limit = \"1\">\n              <i class=\"el-icon-plus\"></i>\n          </el-upload>\n        </el-aside>\n        <el-container>\n          <el-main>Main</el-main>\n          <el-footer>Footer</el-footer>\n        </el-container>\n      </el-container>\n    </el-container>\n    <el-dialog :visible.sync=\"dialogVisible\">\n      <img  :src=\"dialogImageUrl\" alt=\"\">\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  name: 'App',\n\n  data(){\n    return{\n      image:'',\n      dialogImageUrl: '',\n      dialogVisible: false,\n      urls:[\n\n      ]\n    }\n  },\n\n  methods:{\n    getImgUrl(img) {\n      //这里的路径注意自己的后端Controller路径\n      return `http://localhost:8080/common/download?name=${img}`\n    },\n\n    handleRemove(file, fileList) {\n      console.log(file, fileList);\n    },\n    handlePictureCardPreview(file) {\n      this.dialogImageUrl = file.url;\n      this.dialogVisible = true;\n    }\n  }\n}\n</script>\n\n<style>\n.el-header, .el-footer {\n  background-color: #B3C0D1;\n  color: #333;\n  text-align: center;\n  line-height: 60px;\n}\n\n.el-aside {\n  background-color: #D3DCE6;\n  color: #333;\n  text-align: center;\n  line-height: 200px;\n}\n\n.el-main {\n  background-color: #E9EEF3;\n  color: #333;\n  text-align: center;\n  line-height: 100%;\n}\n\nbody > .el-container {\n  margin-bottom: 40px;\n}\n\n.el-container:nth-child(5) .el-aside,\n.el-container:nth-child(6) .el-aside {\n  line-height: 100%;\n}\n\n.el-container:nth-child(7) .el-aside {\n  line-height: 320px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}